{
  "name": "dash-project",
  "version": "1.0.0",
  "description": "## Description",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cd frontend && yarn dev",
    "check-types": "cd frontend && yarn tsc --pretty --noEmit",
    "check-format": "cd frontend && yarn prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "format": "cd frontend && prettier --write .",
    "test-all": "yarn check-format && yarn check-lint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbalthis/dash-project.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "mongodb"
  ],
  "author": "Jason Balthis",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jbalthis/dash-project/issues"
  },
  "homepage": "https://github.com/jbalthis/dash-project#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "husky": "^8.0.3"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "prettier": "^3.0.0"
  }
}
